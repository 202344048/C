#2023-03-30 5주차 c언어 수업
#include<stdio.h>

int main(void)
{
  int A, B;

  printf("정수 두개를 입력하시오:");
  scanf("%d %d", &A, &B);

  if(A>B)
  {printf("A가 B보다 큰 경우에는 '>'를 출력한다.");}

  else if(A<B)
  {printf("A가 B보다 작은 경우에는 '<'를 출력한다.");}

  else if(A==B)

  {printf("A가 B랑 같은 경우에는 '=='를 출력한다.");}

  return 0;
}
